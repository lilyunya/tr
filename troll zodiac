    {
        cout << "Karkat Vantas and Kankri Vantas";
    }
    if ( sign == "Gemini")
    {
        cout << "Sollux Captor and Mituna Captor";
    }
    if ( sign == "Aries")
    {
        cout << "Aradia Megido and Damara Megido";
    }
    if ( sign == "Libra")
    {
        cout << "Terezi Pyrope and Latula Pyrope";
    }
    if ( sign == "Leo")
    {
        cout << "Nepeta Leijon and Meulin Leijon";
    }
    if ( sign == "Virgo")
    {
        cout << "Kanaya Maryam and Porrim Maryam";
    }
    if ( sign == "Taurus")
    {
        cout << "Tavros Nitram and Rufioh Nitram";
    }
